{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "DeepSeek Desktop App",
  "version": "1.0.0",
  "identifier": "com.deepseek.app",
  "build": {
    "frontendDist": "../main",
    "devUrl": "file://../main/index.html",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "DeepSeek Desktop App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
